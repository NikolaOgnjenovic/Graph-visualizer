    <wormholeNetwork id="WN001" name="wormholeNetwork" reference="nebulaCluster">
        <wormhole id="WH001" name="wormhole" refs="blackHoleX" reference="orbitalRing"/>
        <gatewayNode id="GN002" name="gatewayNode" link="wormhole"/>
    </wormholeNetwork>
</galacticNetwork>
