            <planet id="PL001" name="planet" reference="spaceStationAlpha">
                <moon id="MN001" name="moon" ref="orbitalRing" href="cometTail"/>
                <orbitalRing id="OR001" name="orbitalRing" reference="planet"/>
                <cometTail id="CT001" name="cometTail" refs="nebulaCluster"/>
